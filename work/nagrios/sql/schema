CREATE TABLE ACTION (ID VARCHAR(20), NAME VARCHAR(20), FILEPATH VARCHAR(200), HASH VARCHAR(32));
CREATE TABLE SCHEDULE (ID VARCHAR(20), PROCEDURE_ID VARCHAR(20), ASSET_ID VARCHAR(20), ARGUMENT VARCHAR(2000), RECURRENCE VARCHAR(200));
CREATE TABLE EVENT (ID VARCHAR(20), SCHEDULE_ID VARCHAR(20), EXECUTED_TIME DATETIME, RESULT VARCHAR(2000));
CREATE TABLE NOTIFICATION (ID VARCHAR(20), SCHEDULE_ID VARCHAR(20), EXECUTED_TIME DATETIME, CONTACT_ID VARCHAR(20));
CREATE TABLE CONTACT (ID VARCHAR(20), NAME VARCHAR(40), EMAIL VARCHAR(200));
CREATE TABLE ASSET (ID VARCHAR(20), NAME VARCHAR(20));
